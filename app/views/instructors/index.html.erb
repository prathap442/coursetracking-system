  <p id="notice"><%= notice %></p>
  <h1>Instructors</h1>
  <%data={}%> 
  <%total=0%>
  <iframe width="560" height="315" src="https://www.youtube.com/embed/Ksbxik1FpB4" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
  <div class = 'row'>
  <div class = 'col-md-6'>
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Bio</th>
        <th>Courses</th>
        <th colspan="3"></th>
      </tr>
    </thead>
     
          
    <tbody>
      <% @instructors.each do |instructor| %>
        <tr>
          <td><%=link_to instructor.name,instructor,remote: true%></td>
          <td><%=instructor.email%></td>
          <td><%= instructor.bio %></td>
          <td><%instructor.courses.each do |course|%>
              <%total+= course.students.length%>
              <%= course.name %>
              <%data[instructor.name]=total%>
              <%total=0%>
              <%end%>
              </td>  
          <%#= link_to 'Show', instructor %>
          <%#= link_to 'Edit', edit_instructor_path(instructor) %>
          <%#= link_to 'Destroy', instructor, method: :delete, data: { confirm: 'Are you sure?' } %>
        </tr>
      <% end %>
    </tbody>
  </table>
  <br/>
 </div>

 <div class= "col-md-6">
   <%= render 'form',instructor: @instructor %>
 </div>
</div>
 <%=pie_chart(data)%>


<div class="modal fade" id="instructorShowModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle"></h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        ...
      </div>
      
    </div>
  </div>
</div>